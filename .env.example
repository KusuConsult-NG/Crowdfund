# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to Git!

# Appwrite API Key (Server-side only - DO NOT expose to client)
APPWRITE_API_KEY=your_api_key_here

# Appwrite Configuration (Client-side safe)
VITE_APPWRITE_ENDPOINT=https://fra.cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your_project_id_here
VITE_DATABASE_ID=your_database_id_here
VITE_BUCKET_ID=your_bucket_id_here

# Paystack Public Key (Client-side safe)
VITE_PAYSTACK_PUBLIC_KEY=your_paystack_public_key_here
